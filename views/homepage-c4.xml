<odoo>
  <template id="s_homepage_quality_list" name="content quality list">
<section class="centered-content space-between-contents quality-list s_call_to_action o_cc o_colored_level" data-snippet="s_homepage_quality_list" data-name="content quality list">
        <div class="pa-site-width ">
          <div class="row quality-list-c1">
            <div class="col-md-9 pb16 o_colored_level">
              <h3 class="primary-heading fontsize2 o_default_snippet_text">Why are we That Special?</h3>
            </div>
            <div class="col-md-3 pt8 o_colored_level">
              <p class="centered-content">
                <a href="/contactus" class="primary-button o_default_snippet_text">Send Enquiry</a>
              </p>
            </div>
          </div>
          <div class="row align-items-center">
            <div class="col-md-6 o_colored_level">
              <div class="blue-layer-image content-quality-list-img">
                <picture>
                    <source media="(min-width: 991px)" srcset="/theme_pa/static/src/images/PenguinApparel-Building-Web-View.png">
                      <source media="(max-width: 768px)" srcset="/theme_pa/static/src/images/PenguinApparel-Building-Mobile-View.jpg">
                        <img class="img-fluid" src="/theme_pa/static/src/images/PenguinApparel-Building-Web-View.png" alt="Penguin Apparels Building Image" loading="lazy"/>
                      </source>
                    </source>
                </picture>
                <div class="textin-image">
                  <h3 class="primary-heading fontsize2 text-center text-white o_default_snippet_text">Why are we That Special?</h3>
                </div>
              </div>
            </div>
            <div class="col-md-6 pt16 pb16 o_colored_level content_quality_list primary-heading">
              <ul class="two_in_row primary-heading fontsize4">
                <li>
                  <img class="logo-size" src="/theme_pa/static/src/images/logos/nedle.png" loading="lazy" alt="Needle Logo"/>
                </li>
                <li class="o_default_snippet_text"> Design and Development</li>
              </ul>
              <ul class="two_in_row primary-heading fontsize4">
                <li>
                  <img class="logo-size" src="/theme_pa/static/src/images/logos/procces.png" loading="lazy" alt="shirt logo"/>
                </li>
                <li class="o_default_snippet_text"> Flexible and Minimums</li>
              </ul>
              
              <ul class="two_in_row primary-heading fontsize4">
                <li>
                  <img class="logo-size" src="/theme_pa/static/src/images/logos/folded.png" loading="lazy" alt="cloth logo"/>
                </li>
                <li class="o_default_snippet_text"> Fabric Sourcing and Development</li>
              </ul>
              <ul class="two_in_row primary-heading fontsize4">
                <li>
                  <img class="logo-size" src="/theme_pa/static/src/images/logos/Inspect-Logo.png" loading="lazy" alt="inspect logo"/>
                </li>
                <li class="o_default_snippet_text"> Prototyping and Samplings</li>
              </ul>
              <ul class="two_in_row primary-heading fontsize4">
                <li>
                  <img class="logo-size" src="/theme_pa/static/src/images/logos/shirt.png" loading="lazy" alt="shirt logo image"/>
                </li>
                <li class="o_default_snippet_text"> Pattern development</li>
              </ul>
              <ul class="two_in_row primary-heading fontsize4">
                <li>
                  <img class="logo-size" src="/theme_pa/static/src/images/logos/timekeep.png" alt="Time logo" loading="lazy"/>
                </li>
                <li class="o_default_snippet_text"> Quick Order Fullfillment</li>
              </ul>
              <ul class="two_in_row primary-heading fontsize4">
                <li>
                  <img class="logo-size" src="/theme_pa/static/src/images/logos/protection.png" alt="protection logo" loading="lazy"/>
                </li>
                <li class="o_default_snippet_text"> Trusted Quality Standards</li>
              </ul>
            </div>
          </div>
        </div>
        <div class="col-md-3 pt8 o_colored_level quality-list-c2">
          <p class="centered-content">
            <a href="/contactus" class="primary-button o_default_snippet_text">Send Enquery</a>
          </p>
        </div>
      </section>
</template>
<template id="place_into_hd" inherit_id="website.snippets" name="Place into hd">
    <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
        <t t-snippet="theme_pa.s_homepage_quality_list" t-thumbnail="/theme_pa/static/src/images/logos/PenguinApparels-Logo.png"/>
    </xpath>
</template>
</odoo>