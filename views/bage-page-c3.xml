          <div class="row align-items-center">
            <div class="col-lg-6 pt16 pb16 o_colored_level">
              <picture>
              <source media="(max-width: 991px) and (min-width: 768px)" srcset="/theme_Advance/static/src/images/Employee-Training-Tablet-View.png">
                <source media="(min-width: 991px)" srcset="/theme_Advance/static/src/images/Employee-Training-Web-View.png">
