</template>
<template id="place_into_page_content_patrner" inherit_id="website.snippets" name="Place into page partners">
    <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
        <t t-snippet="theme_pa.s_process_page_content_partners" t-thumbnail="/theme_pa/static/src/images/logos/procces.png"/>
    </xpath>
</template>
</odoo>
   
