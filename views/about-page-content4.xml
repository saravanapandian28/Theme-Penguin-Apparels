<odoo>
<template id="s_content_aboutus_export" name="company export details">
<section class="space-between-contents bg-200" data-snippet="s_content_aboutus_export" data-name="company export details">
        <div class=" export-detail">
          <div class="pa-site-width">
            <div class="row centered-content">
              <div class="col-md-4 o_colored_level export-detail-c">
                <h3 class="primary-heading pb16 o_default_snippet_text fontsize7">$51.8 M</h3>
                <p class="light-font o_default_snippet_text fontsize5 export-detail-head">GARMENTS EXPORTS</p>
                <p class="primary-subtitle o_default_snippet_text fontsize3">Years 2016 - 2018</p>
              </div>
              <div class="col-md-4 o_colored_level export-detail-c">
                <h3 class="primary-heading pb16 o_default_snippet_text fontsize7">100+</h3>
                <p class="light-font o_default_snippet_text fontsize5 export-detail-head">CLIENTS</p>
                <p class="primary-subtitle o_default_snippet_text fontsize3 export-detail-head">Across The Globe</p>
              </div>
              <div class="col-md-4 o_colored_level export-detail-c">
                <h3 class="primary-heading pb16 o_default_snippet_text fontsize7">1000+</h3>
                <p class="light-font o_default_snippet_text fontsize5 export-detail-head">EXPORTS</p>
                <p class="primary-subtitle o_default_snippet_text fontsize3">All Location</p>
              </div>
            </div>
          </div>
        </div>
      </section>


<template id="place_into_aboutus" inherit_id="website.snippets" name="Place into aboutus page">
    <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
        <t t-snippet="theme_pa.s_content_aboutus_export" t-thumbnail="/theme_pa/static/src/images/logos/Inspect-Logo.png"/>
    </xpath>
</template>
</odoo>